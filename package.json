{
  "name": "@enslo/sd-metadata",
  "version": "1.7.1",
  "packageManager": "pnpm@10.5.2",
  "description": "Read and write AI-generated image metadata",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "markdownlint": "markdownlint --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "lefthook install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enslo/sd-metadata.git"
  },
  "keywords": [
    "stable-diffusion",
    "ai-image",
    "image-metadata",
    "metadata-parser",
    "prompt",
    "novelai",
    "comfyui",
    "forge",
    "png",
    "jpeg",
    "webp",
    "exif"
  ],
  "author": "Ernest Croft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enslo/sd-metadata/issues"
  },
  "homepage": "https://sd-metadata.pages.dev",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.17",
    "lefthook": "^1.10.10",
    "markdownlint-cli": "^0.47.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^4.0.17"
  }
}
