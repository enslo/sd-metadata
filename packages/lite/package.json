{
  "name": "@enslo/sd-metadata-lite",
  "version": "1.0.0",
  "description": "Lightweight AI image metadata reader for bookmarklets",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.ja.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup && tsx build-iife.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enslo/sd-metadata.git",
    "directory": "packages/lite"
  },
  "keywords": [
    "stable-diffusion",
    "ai-image",
    "metadata-parser",
    "lightweight",
    "bookmarklet"
  ],
  "author": "Ernest Croft",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/node": "^25.0.3",
    "esbuild": "^0.25.0",
    "terser": "^5.46.0",
    "tsup": "^8.5.1",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
